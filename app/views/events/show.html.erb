<h2>Event created by <%= @event.creator.name %> </h2>
<%= @event.title %><br>
<br><%= @event.date %>

<ul>
  <% @event_attendees.errors.full_messages.each do |msg| %>
    <li><%= msg %></li>
  <% end %>
</ul>
<ul>
  <% @event_attendees.each do |attendee| %>
    <li>
      <%= attendee.name %>
    </li>
  <% end %>
</ul>
<%= button_to 'Attend Event', edit_event_path(@event), method: :get %>
<%= link_to 'Back to Events', events_path%>